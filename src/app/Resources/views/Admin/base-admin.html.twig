<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta charset="UTF-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    <title>{% block title %}{% endblock %}{% if block('title') %} - {% endif %}{{ site_name }}</title>

    <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,300,100italic,500,500italic,300italic,400italic,700,700italic' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Droid+Serif:400,400italic,700,700italic' rel='stylesheet' type='text/css'>

    {% stylesheets
        'bundles/dwiasset/css/flaticons/stroke.css'
        'bundles/dwiasset/css/idangerous.swiper.css'
        'bundles/dwiasset/css/medium-editor.css'
        'bundles/dwiasset/css/jquery.dropdown.css'
        'bundles/dwiasset/css/dwi.css'
        filter='cssrewrite' combine=true output='css/assets.css'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    {% block stylesheets %}{% endblock %}
</head>
<body class="admin">
    {% include ':Admin:sidebar.html.twig' %}

    <section class="content">
        {% block body %}{% endblock %}
    </section>

    <script src='{{ asset('//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js') }}' type='text/javascript'></script>
    <script src='{{ asset('bundles/dwiasset/js/idangerous.swiper-2.4.1.min.js') }}' type='text/javascript'></script>
    <script src="{{ asset('bundles/dwiasset/js/medium-editor.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/dwiasset/js/dwi.js') }}" type="text/javascript"></script>

    {% block javascript %}{% endblock %}
</body>
</html>
